<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_wizard_manual_import_booking" model="ir.ui.view">
            <field name="model">wizard.manual.import.booking</field>
            <field name="arch" type="xml">
                <form string="Manual Update Bookings">
                    <h3>Import Booking from xml</h3>
                        <group>
                            <group>
                                <field name="ffile" filename="file_name"/>
                            </group>
                         </group>
                    <footer>
                        <button name="button_ok" class="oe_highlight"
                            type="object" string="Ok"/>
                        or
                        <button class="oe_link" special="cancel" string="Cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</openerp>
