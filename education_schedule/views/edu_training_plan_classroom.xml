<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="edu_training_plan_classroom_form_view" model="ir.ui.view">
            <field name="model">edu.training.plan.classroom</field>
            <field name="inherit_id" ref="education.edu_training_plan_classroom_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page name="time_slots" string="Time Slots">
                        <field name="time_slot_ids"/>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
