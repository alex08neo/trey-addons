msgid "\n"
"<table id=\"backgroundTable\" cellpadding=\"0\" cellspacing=\"0\" border=\"0\" style=\"margin: 0;padding: 0;width: 100% !important;line-height: 100% !important;border-collapse: collapse;mso-table-lspace: 0;mso-table-rspace: 0;border:1px solid grey;\">\n"
