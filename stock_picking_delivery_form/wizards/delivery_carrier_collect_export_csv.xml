<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="delivery_carrier_collect_export_csv_view" model="ir.ui.view">
            <field name="model">delivery.carrier.collect_export_csv</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Export media delivery">
                    <group colspan="4">
                        <field name="file" nolabel="1" filename="file_name"/>
                        <field name="file_name" invisible="True"/>
                    </group>
                    <footer>
                        <button class="oe_highlight" special="cancel" string="Accept"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</openerp>
