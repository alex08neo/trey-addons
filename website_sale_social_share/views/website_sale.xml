<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <template id="product" inherit_id="website_sale.product" name="Social share" customize_show="True" active="False">
            <xpath expr="//*[contains(@class, 'js_add_cart_variants')]/../*[last()]" position="after">
                <div class="social-share-buttons mt16" data-bind="social-share-buttons"></div>
            </xpath>
        </template>

    </data>
</openerp>
